 <h2>User info</h2>
      <table class='table'>
        <tr>
          <img class="attachment user profile_image fallback" src="assets/images/no_image.jpg" />
        </tr>
        <tr>
          <th>name</th>
	        <th><%= current_user.name%></th>
        </tr>
        <tr>
	        <th>introduction</th>
	        <th><%= current_user.email%></th>
        </tr>
      </table>


      <div class='row'>
        <p><%= link_to "プロフィール編集", edit_user_registration_path(current_user), {class: "btn btn-outline-secondary btn-block fas fa-user-cog edit_user_20259"} %></p>
      </div>

      <h2 class="mt-3">New book</h2>
      <%= form_with model: Book.new, local:true do |f| %>
        <h4>title</h4>
        <%= f.text_field :title %>
        <h4>body</h4>
        <%= f.text_area :body %>
        <%= f.submit '投稿' %>
      <% end %>